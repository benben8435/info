<%= form_for(@dinning) do |f| %>
  <% if @dinning.errors.any? %>
    <div class="alert alert-warning">
      <h2><%= pluralize(@dinning.errors.count, "error") %> prohibited this dinning from being saved:</h2>

      <ul>
      <% @dinning.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
<div class="row">
  <div class="col-md-3"></div>
  <div class="col-md-6">
    <div class="row top30"></div>
    <div class="form-horizontal">

      <div class="form-group">
        <%= f.label :quantity, class: "col-sm-3 control-label" %>
        <div class="col-sm-9">
        <%= f.text_field :quantity, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :price, class: "col-sm-3 control-label" %>
        <div class="col-sm-9">
        <%= f.text_field :price, class: "form-control" %>
        </div>
      </div>
      <br>
      <div class="form-group text-right">
        <%= f.submit 'Submit', class: "btn btn-primary"  %>
        <%= link_to 'Back', :back, class: "btn btn-default" %>
      </div>
    </div>
  </div>
</div>
<% end %>
